#!/bin/sh

CC=tcc
BIN_DIR=target/bin

mkdir -p ${BIN_DIR};

COMMAND="${CC} -O3 \
	-march=armv8-a \
	-fno-stack-clash-protection \
	-fno-builtin-memset \
	-nostdlib \
	-std=c11 \
	-pedantic \
	-Werror \
	-Wall \
	-Wextra \
	-Wshadow \
	-Wpointer-arith \
	-Wcast-qual \
	-Wmissing-prototypes \
	-Wmissing-declarations \
	-o ${BIN_DIR}/test \
	main.c"

#COMMAND="${CC} -fvisibility=hidden -nostdlib -shared -fPIC -O3 -mavx2 -DCOMPRESS_IMPL -x c include/compress.h -o libcompress.so"

echo ${COMMAND};
${COMMAND};
